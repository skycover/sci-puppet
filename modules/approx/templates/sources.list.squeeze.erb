<% if @local_sources == "yes" %>
deb http://apt.<%= @domain %>:9999/cdrom <%= @lsbdistcodename %> main
<% end %>
deb http://apt.<%= @domain %>:9999/archive <%= @lsbdistcodename %> main non-free contrib
deb http://apt.<%= @domain %>:9999/debian <%= @lsbdistcodename %>-lts main non-free contrib
deb http://apt.<%= @domain %>:9999/backports <%= @lsbdistcodename %>-backports main non-free contrib

